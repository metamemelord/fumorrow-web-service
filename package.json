      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "dependencies": {
    "assert": "^1.4.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cloudinary": "^1.13.2",
    "crypto": "^1.0.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "log4js": "^3.0.5",
    "md5": "^2.2.1",
    "mocha": "^5.2.0",
    "moment": "^2.23.0",
    "mongoose": "^5.2.14",
    "mysql2": "^1.6.1"
