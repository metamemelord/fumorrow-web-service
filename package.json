{
  "name": "fumorrow",
  "version": "2.3.0",
  "description": "Know about upcoming things.",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon ./index.js"
  },
  "keywords": [
    "Fumorrow",
    "Future",
    "Coming",
    "Soon",
    "Tomorrow"
  ],
  "author": "Gaurav Saini",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        "tab"
